{
  "name": "optimistdigital/nova-drafts",
  "description": "A Laravel Nova field that allows you to make drafts of your current resources.",
  "keywords": [
    "laravel",
    "nova",
    "optimistdigital",
    "drafts",
    "field"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.1.0",
    "laravel/nova": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "OptimistDigital\\NovaDrafts\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "OptimistDigital\\NovaDrafts\\FieldServiceProvider"
      ]
    }
  },
  "repositories": [
    {
        "type": "composer",
        "url": "https://nova.laravel.com"
    }
  ],
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
